<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_navigation_notifications"
            app:destination="@id/navigation_notifications" />
        <action
            android:id="@+id/action_navigation_home_to_tesiFragmant"
            app:destination="@id/tesiFragmant" />
        <action
            android:id="@+id/action_navigation_home_to_lista_tesi"
            app:destination="@id/lista_tesi" />
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" >
        <action
            android:id="@+id/action_navigation_notifications_to_chatFragment"
            app:destination="@id/chatFragment" />
        <action
            android:id="@+id/action_navigation_notifications_to_ticketFragment"
            app:destination="@id/ticketFragment" />
        <action
            android:id="@+id/action_navigation_notifications_to_bookingFragment"
            app:destination="@id/bookingFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />
    <fragment
        android:id="@+id/tesiFragmant"
        android:name="com.uniba.mobile.cddgl.laureapp.TesiFragment"
        android:label="@string/crea_tesi"
        tools:layout="@layout/fragment_tesi" >
        <action
            android:id="@+id/action_tesiFragmant_to_navigation_home"
            app:destination="@id/navigation_home" />
    </fragment>
    <fragment
        android:id="@+id/lista_tesi"
        android:name="com.uniba.mobile.cddgl.laureapp.lista_tesi"
        android:label="fragment_lista_tesi"
        tools:layout="@layout/fragment_lista_tesi" />
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.chat.ChatFragment"
        android:label="@string/title_chat"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/nav_chat_list_fragment"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.chat.ChatListFragment"
        android:label="ChatListFragment" >
        <action
            android:id="@+id/action_chatListFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_ticket"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.ticket.TicketListFragment"
        android:label="@string/title_tickets"
        tools:layout="@layout/fragment_ticket_list" >
        <action
            android:id="@+id/action_nav_ticket_to_ticketFragment"
            app:destination="@id/ticketFragment" />
    </fragment>
    <fragment
        android:id="@+id/ticketFragment"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.ticket.TicketFragment"
        android:label="@string/title_ticket"
        tools:layout="@layout/fragment_ticket" />
    <fragment
        android:id="@+id/visualizeTesiFragment"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.tesi.VisualizeTesiFragment"
        android:label=""
        tools:layout="@layout/fragment_visualize_tesi" >
        <action
            android:id="@+id/action_visualizeTesiFragment_to_ticketFragment"
            app:destination="@id/ticketFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_bookingListFragment"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.bookings.BookingListFragment"
        android:label="@string/booking_list"
        tools:layout="@layout/fragment_booking_list" >
        <action
            android:id="@+id/action_nav_bookingListFragment_to_bookingFragment"
            app:destination="@id/bookingFragment" />
    </fragment>
    <fragment
        android:id="@+id/bookingFragment"
        android:name="com.uniba.mobile.cddgl.laureapp.ui.bookings.BookingFragment"
        android:label="@string/booking" />
    <fragment
        android:id="@+id/logo_fragment"
        android:name="com.uniba.mobile.cddgl.laureapp.LogoFragment"
        android:label="fragment_logo"
        tools:layout="@layout/fragment_logo" >
        <action
            android:id="@+id/action_logo_fragment_to_navigation_home"
            app:destination="@id/navigation_home"
            app:popUpTo="@id/logo_fragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/ricevimento"
        android:name="com.uniba.mobile.cddgl.laureapp.RicevimentoFragment"
        android:label="fragment_ricevimento"
        tools:layout="@layout/fragment_ricevimento" />
</navigation>